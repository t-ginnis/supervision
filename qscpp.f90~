program supoq3
  implicit none
  integer,parameter :: N=100,dp=8
  real (kind=dp):: x(N) , y(N) , A(N,N) , D(N,N)

  ! Initialize x and y
  do j=1,N
     x(j)=cos(real(j,dp))
     y(j)=cos(2*real(j,dp))
     do i=1,N
        A(i,j)=sin(real(i-j+1,dp))
     end do
  end do
  

  print "(a12)","Initialised"
  
        


end program supoq3
